
config dnsmasq
	option domainneeded '1'
	option localise_queries '1'
	option rebind_protection '0'
	option local '/lan/'
	option domain 'lan'
	option expandhosts '1'
	option use_stale_cache '3600'
	option nonegcache '1'
	option authoritative '1'
	option leasefile '/tmp/dhcp.leases'
	option localservice '0'
	option ednspacket_max '1232'
	option filter_aaaa '1'
	option sequential_ip '1'
	option allservers '1'
	option ignore_hosts_dir '1'
	list interface 'lan'
	option strictorder '1'
	option dnsforwardmax '4096'
	option cachesize '4096'
	option quietdhcp '1'
	option localuse '1'
	option port '0'
	option old_dns_redirect '1'
	option min_ttl '3600'
	option readethers '1'
	option noresolv '1'
	option nohosts '1'
	option dnssec '1'

config dhcp 'lan'
	option interface 'lan'
	option start '100'
	option limit '150'
	option leasetime '12h'
	option dhcpv4 'server'
	option force '1'
	list dhcp_option '6,192.168.3.2'
	list dhcp_option '6,192.168.3.2'
	option ra 'hybrid'
	option ndp 'hybrid'
	option dhcpv6 'hybrid'

config odhcpd 'odhcpd'
	option maindhcp '0'
	option leasefile '/tmp/hosts/odhcpd'
	option leasetrigger '/usr/sbin/odhcpd-update'
	option loglevel '4'

config domain '_sc_pw'
	option name 'pw'
	option ip '10.0.0.1'
	option comments 'PassWall'

config domain '_ta'
	option name 'time.android.com'
	option ip '203.107.6.88'
	option comments 'time.android'

config domain 'default_server'
	option name 'kwrt'
	option ip '10.0.0.1'
	option comments '后台地址'

