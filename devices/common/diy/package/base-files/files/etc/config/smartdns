
config smartdns
	option enabled '1'
	option server_name 'smartdns'
	option port '53'
	option auto_set_dnsmasq '1'
	option tcp_server '0'
	option tls_server '0'
	option doh_server '0'
	option ipv6_server '0'
	option bind_device '0'
	option dualstack_ip_selection '0'
	option serve_expired '0'
	option cache_persist '0'
	option resolve_local_hostnames '0'
	option force_https_soa '0'
	option rr_ttl_min '600'
	option seconddns_port '6553'
	option seconddns_tcp_server '0'
	option log_output_mode 'file'
	option enable_auto_update '1'
	option auto_update_week_time '6'
	option auto_update_day_time '5'
	option old_port '53'
	option old_enabled '1'
	option old_auto_set_dnsmasq '1'

config domain-rule

config client-rule
	option enabled '0'
	option dualstack_ip_selection '0'
	option force_https_soa '0'

config ip-rule

config download-file
	option name 'anti-ad-smartdns.conf'
	option url 'https://anti-ad.net/anti-ad-for-smartdns.conf'
	option type 'config'
	option desc '屏蔽广告'

config download-file
	option name 'domains.china.smartdns.conf'
	option type 'list'
	option desc '国内域名'
	option url 'https://raw.githubusercontent.com/leesuncom/NetGearR7000/refs/heads/main/common/etc/smartdns/domain-set/domains.china.smartdns.conf'

config download-file
	option name 'proxy-domain-list.conf'
	option url 'https://raw.githubusercontent.com/leesuncom/NetGearR7000/refs/heads/main/common/etc/smartdns/domain-set/proxy-domain-list.conf'
	option type 'list'
	option desc '国外域名'

